https://leetcode.com/problems/remove-element/description/

class Solution {
public:
    int removeElement(vector<int>& nums, int val) 
    {
        //2 pointer 
        int slow=0;
        int fast=0;
        int n=nums.size();

        while(fast<n)
        {
            if(nums[fast] != val)
            {
                swap(nums[slow],nums[fast]);
                slow++;
            }
            fast++;
        }
        return slow;
    }
};

