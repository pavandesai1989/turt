https://neetcode.io/problems/search-insert-position/question?list=allNC

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) 
    {
        int ans=0;
        int n=nums.size();
        int l=0,r=n-1;

        while(l<=r)
        {
           int mid=(r-l)/2+l;

           if(nums[mid]<target)
           {
              l = mid+1;
           }
           else
           {
              r = mid-1;
           }
        }
        return l;//ele will be here 
    }
};
