https://leetcode.com/problems/pascals-triangle-ii/submissions/1866736713/

class Solution {
public:
    vector<int> getRow(int rowIndex) 
    {
        vector<int> prev;
        for(int i=0;i<=rowIndex;i++) //0 based idx vvvimp
        {
            vector<int> curr(i+1,1);
            for(int j=1;j<i;j++)
            {
                curr[j] = prev[j] + prev[j-1];
            }
            prev = curr;
        }
        return prev;
    }
};
